{
    "name": "example-tick",
    "services": [
        {
            "type": "plugin.common.timer.tick",
            "uri": "tick",
            "port_type": { "Source": 1 },
            "config": {
                "interval_millis": 1000,
                "start": 5,
                "end": 10
            },
            "wires": ["broadcast"]
        },
        {
            "type": "plugin.common.fanout.broadcast",
            "uri": "broadcast",
            "port_type": { "Flow": [1, 2] },
            "config": {
            },
            "wires": ["last1", "last2"]
        },
        {
            "type": "plugin.common.collect.last",
            "uri": "last1",
            "port_type": { "Sink": 1 },
            "config": {
            },
            "wires": []
        },
        {
            "type": "plugin.common.collect.last",
            "uri": "last2",
            "port_type": { "Sink": 1 },
            "config": {
            },
            "wires": []
        }
    ]
}
